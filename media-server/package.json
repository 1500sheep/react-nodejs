{
  "name": "prayonserver",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npx nodemon ./bin/www --exec babel-node localhost",
    "initdb": "npx babel-node src/common/database.js initdb",
    "reloaddb": "rm db.sqlite && npm run initdb && npm run dump",
    "dump": "babel-node src/common/dump.js",
    "pm2": "pm2 start app.js —interpreter ./node_modules/.bin/babel-node —name 'gidon-server'"
  },
