    "co-fs-extra": "^1.2.1",
    "co-multipart": "^2.0.0",
    "connect-multiparty": "^2.1.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "ffprobe": "^1.1.0",
    "form-data": "^2.3.1",
    "get-audio-duration": "0.0.1",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.1.0",
    "jszip": "^3.1.5",
    "moment": "^2.19.1",
    "morgan": "^1.8.2",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.1",
    "node-fetch": "^1.7.3",
